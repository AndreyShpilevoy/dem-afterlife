{
    "name": "deus-ex-machina-frontend",
    "version": "0.0.2",
    "description": "Frontend part for DeusExMachina website",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/AndreyShpilevoy/dem-afterlife"
    },
    "scripts": {
        "setEnv": "npm run setDevEnv && npm install -g npm-check-updates",
        "setDevEnv": "npm config set production false && npm config set dev true",
        "setProdEnv": "npm config set production true && npm config set dev false",
        "clean-wwwroot": "rimraf ../dem-afterlife/wwwroot && mkdirp ../dem-afterlife/wwwroot",
        "test-cached": "cross-env BABEL_ENV=test jest",
        "test": "cross-env BABEL_ENV=test jest --no-cache",
        "prebuild": "yarn run clean-wwwroot && yarn run test-cached",
        "build": "cross-env BABEL_ENV=production babel-node tools/distServer.js",
        "prestart": "yarn run test-cached",
        "start": "cross-env BABEL_ENV=development babel-node tools/srcServer.js"
    },
    "jest": {
        "coverageThreshold": {
            "global": {
                "statements": 80,
                "branches": 80,
                "functions": 80,
                "lines": 80
            }
        },
        "collectCoverage": true,
        "testRegex": "src/.*/.*test.js",
        "transform": {
            ".*": "<rootDir>/tools/jestPreProcessor"
        },
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js"
        },
        "moduleFileExtensions": [
            "js",
            "es6",
            "jsx",
            "json"
        ],
        "testPathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "/__snapshots__/",
            "/__mocks__/",
            "/__fakeApi__/",
            "/__fakeData__/"
        ],
        "coveragePathIgnorePatterns": [
            "src/.+\\.test\\.js",
            "<rootDir>/node_modules/",
            "/__snapshots__/",
            "/__mocks__/",
            "/__fakeApi__/",
            "/__fakeData__/",
            "src/scripts/styles/",
            "src/images/",
            "tools/"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    },
    "devDependencies": {
        "babel-cli": "6.24.1",
        "babel-core": "6.24.1",
        "babel-eslint": "7.2.3",
        "babel-jest": "20.0.3",
        "babel-loader": "7.0.0",
        "babel-plugin-lodash": "3.2.11",
        "babel-plugin-ramda": "1.2.0",
        "babel-plugin-transform-class-properties": "6.24.1",
        "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
        "babel-plugin-transform-react-remove-prop-types": "0.4.5",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-preset-es2015": "6.24.1",
        "babel-preset-react": "6.24.1",
        "babel-preset-stage-3": "6.24.1",
        "checksum": "0.1.1",
        "colors": "1.1.2",
        "copy-webpack-plugin": "4.0.1",
        "cross-env": "5.0.1",
        "enzyme": "2.8.2",
        "enzyme-to-json": "1.5.1",
        "eslint": "3.19.0",
        "eslint-config-airbnb-base": "11.2.0",
        "eslint-import-resolver-webpack": "0.8.1",
        "eslint-loader": "1.7.1",
        "eslint-plugin-fp": "2.3.0",
        "eslint-plugin-import": "2.3.0",
        "eslint-plugin-react": "7.0.1",
        "express": "4.15.3",
        "file-loader": "0.11.2",
        "html-webpack-plugin": "2.28.0",
        "jest": "20.0.4",
        "jsdom": "11.0.0",
        "mkdirp": "0.5.1",
        "open": "0.0.5",
        "react-hot-loader": "3.0.0-beta.6",
        "react-test-renderer": "15.5.4",
        "redux-immutable-state-invariant": "2.0.0",
        "redux-mock-store": "1.2.3",
        "rimraf": "2.6.1",
        "transform-jest-deps": "2.2.1",
        "url-loader": "0.5.8",
        "webpack": "2.6.1",
        "webpack-dev-middleware": "1.10.2",
        "webpack-hot-middleware": "2.18.0"
    },
    "dependencies": {
        "aesthetic": "1.4.0",
        "aesthetic-adapter-jss": "1.4.0",
        "babel-polyfill": "6.23.0",
        "history": "4.6.1",
        "jss": "7.1.5",
        "jss-camel-case": "4.0.0",
        "jss-default-unit": "6.1.2",
        "jss-expand": "3.0.1",
        "jss-extend": "4.0.1",
        "jss-global": "1.0.1",
        "jss-nested": "4.0.1",
        "jss-props-sort": "4.0.0",
        "jss-vendor-prefixer": "5.1.0",
        "lodash._root": "3.0.1",
        "lodash.debounce": "4.0.8",
        "lodash.throttle": "4.1.1",
        "pace-progress": "1.0.2",
        "prop-types": "15.5.10",
        "ramda": "0.24.1",
        "raven-js": "3.15.0",
        "raw-loader": "0.5.1",
        "react": "15.5.4",
        "react-dom": "15.5.4",
        "react-notification-system": "0.2.14",
        "react-redux": "5.0.5",
        "react-router-dom": "4.1.1",
        "react-router-redux": "5.0.0-alpha.6",
        "react-svg-inline": "2.0.0",
        "redux": "3.6.0",
        "redux-saga": "0.15.3"
    }
}
