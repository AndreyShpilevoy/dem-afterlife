{
    "name": "deus-ex-machina-frontend",
    "version": "0.0.2",
    "description": "Frontend part for DeusExMachina website",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/AndreyShpilevoy/dem-afterlife"
    },
    "scripts": {
        "setEnv": "npm run setDevEnv && npm install -g npm-check-updates",
        "setDevEnv": "npm config set production false && npm config set dev true",
        "setProdEnv": "npm config set production true && npm config set dev false",
        "clean-wwwroot": "rimraf ../dem-afterlife/wwwroot && mkdirp ../dem-afterlife/wwwroot",
        "lint": "eslint --color webpack.config.* src tools",
        "lint-fix": "eslint --fix --color webpack.config.* src tools",
        "test": "cross-env BABEL_ENV=test jest --no-cache",
        "prebuild": "yarn run clean-wwwroot && yarn run lint && yarn run test",
        "build": "cross-env BABEL_ENV=production babel-node tools/distServer.js",
        "prestart": "yarn run test",
        "start": "cross-env BABEL_ENV=development babel-node tools/srcServer.js"
    },
    "jest": {
        "collectCoverage": true,
        "testRegex": "src/.+\\.test\\.js",
        "transform": {
            ".*": "<rootDir>/node_modules/babel-jest"
        },
        "moduleFileExtensions": [
            "js",
            "es6",
            "jsx",
            "json"
        ],
        "testPathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "/__mocks__/"
        ],
        "coveragePathIgnorePatterns": [
            "src/.+\\.test\\.js",
            "<rootDir>/node_modules/",
            "/__mocks__/"
        ]
    },
    "devDependencies": {
        "babel-cli": "6.24.0",
        "babel-core": "6.24.0",
        "babel-eslint": "7.2.1",
        "babel-jest": "19.0.0",
        "babel-loader": "6.4.1",
        "babel-plugin-lodash": "3.2.11",
        "babel-plugin-module-resolver": "2.5.0",
        "babel-plugin-transform-class-properties": "6.23.0",
        "babel-plugin-transform-es2015-modules-commonjs": "6.24.0",
        "babel-plugin-transform-react-remove-prop-types": "0.3.2",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-preset-es2015": "6.24.0",
        "babel-preset-react": "6.23.0",
        "babel-preset-stage-0": "6.22.0",
        "babel-preset-stage-1": "6.22.0",
        "babel-preset-stage-2": "6.22.0",
        "babel-preset-stage-3": "6.22.0",
        "checksum": "0.1.1",
        "colors": "1.1.2",
        "copy-webpack-plugin": "4.0.1",
        "cross-env": "3.2.4",
        "enzyme": "2.7.1",
        "eslint": "3.18.0",
        "eslint-config-airbnb-base": "11.1.1",
        "eslint-import-resolver-babel-module": "3.0.0",
        "eslint-loader": "1.7.0",
        "eslint-plugin-import": "2.2.0",
        "eslint-plugin-react": "6.10.3",
        "express": "4.15.2",
        "file-loader": "0.10.1",
        "html-webpack-plugin": "2.28.0",
        "jest-cli": "19.0.2",
        "jest-css-modules": "1.1.0",
        "jsdom": "9.12.0",
        "mkdirp": "0.5.1",
        "open": "0.0.5",
        "react-hot-loader": "3.0.0-beta.6",
        "redux-immutable-state-invariant": "2.0.0",
        "redux-mock-store": "1.2.2",
        "rimraf": "2.6.1",
        "url-loader": "0.5.8",
        "webpack": "2.3.1",
        "webpack-dev-middleware": "1.10.1",
        "webpack-hot-middleware": "2.17.1"
    },
    "dependencies": {
        "aesthetic": "1.1.2",
        "aesthetic-adapter-jss": "1.1.2",
        "babel-polyfill": "6.23.0",
        "history": "4.6.1",
        "jss": "6.5.0",
        "jss-camel-case": "3.0.0",
        "jss-default-unit": "5.0.3",
        "jss-expand": "2.1.0",
        "jss-extend": "3.0.1",
        "jss-nested": "3.0.1",
        "jss-props-sort": "3.0.0",
        "jss-vendor-prefixer": "4.0.0",
        "lodash": "4.17.4",
        "pace-progress": "1.0.2",
        "raven-js": "3.13.1",
        "react": "15.4.2",
        "react-dom": "15.4.2",
        "react-notification-system": "0.2.13",
        "react-redux": "5.0.3",
        "react-router-dom": "4.0.0",
        "react-router-redux": "5.0.0-alpha.3",
        "redux": "3.6.0",
        "redux-saga": "0.14.3"
    }
}
