{
    "name": "deus-ex-machina-frontend",
    "version": "0.0.2",
    "description": "Frontend part for DeusExMachina website",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://github.com/AndreyShpilevoy/dem-afterlife"
    },
    "scripts": {
        "setEnv": "npm run setDevEnv && npm install -g npm-check-updates",
        "setDevEnv": "npm config set production false && npm config set dev true",
        "setProdEnv": "npm config set production true && npm config set dev false",
        "clean-wwwroot": "rimraf ../dem-afterlife/wwwroot && mkdirp ../dem-afterlife/wwwroot",
        "test": "cross-env BABEL_ENV=test jest --config jest.config.json",
        "test-no-cache": "cross-env BABEL_ENV=test jest --config jest.config.json --no-cache",
        "test-runInBand": "cross-env BABEL_ENV=test jest --config jest.config.json --runInBand",
        "buildOnly": "cross-env BABEL_ENV=production webpack --progress --colors",
        "prebuild": "yarn run clean-wwwroot && yarn run test",
        "build": "cross-env BABEL_ENV=production babel-node --presets es2015 tools/distServer.js",
        "prestart": "yarn run test",
        "start": "cross-env BABEL_ENV=development babel-node --presets es2015 tools/srcServer.js"
    },
    "quokka": {
        "babel": true,
        "plugins": [
            "./tools/quokkaAliasPreProcessor"
        ]
    },
    "devDependencies": {
        "babel-cli": "6.26.0",
        "babel-core": "6.26.0",
        "babel-eslint": "8.0.1",
        "babel-jest": "21.2.0",
        "babel-loader": "7.1.2",
        "babel-plugin-lodash": "3.2.11",
        "babel-plugin-ramda": "1.4.3",
        "babel-plugin-tailcall-optimization": "1.0.11",
        "babel-plugin-transform-class-properties": "6.24.1",
        "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
        "babel-plugin-transform-react-constant-elements": "6.23.0",
        "babel-plugin-transform-react-inline-elements": "6.22.0",
        "babel-plugin-transform-react-remove-prop-types": "0.4.8",
        "babel-plugin-transform-runtime": "6.23.0",
        "babel-preset-es2015": "6.24.1",
        "babel-preset-react": "6.24.1",
        "babel-preset-stage-3": "6.24.1",
        "checksum": "0.1.1",
        "colors": "1.1.2",
        "compression-webpack-plugin": "1.0.0",
        "copy-webpack-plugin": "4.0.1",
        "cross-env": "5.0.5",
        "enzyme": "3.0.0",
        "enzyme-adapter-react-16": "1.0.0",
        "enzyme-to-json": "3.0.1",
        "eslint": "4.7.2",
        "eslint-config-airbnb": "15.1.0",
        "eslint-import-resolver-webpack": "0.8.3",
        "eslint-loader": "1.9.0",
        "eslint-plugin-fp": "2.3.0",
        "eslint-plugin-import": "2.7.0",
        "eslint-plugin-jsx-a11y": "5.1.1",
        "eslint-plugin-react": "7.4.0",
        "express": "4.16.0",
        "file-loader": "0.11.2",
        "html-webpack-plugin": "2.30.1",
        "jest": "21.2.1",
        "jsdom": "11.2.0",
        "mkdirp": "0.5.1",
        "module": "1.2.5",
        "open": "0.0.5",
        "raw-loader": "0.5.1",
        "react-hot-loader": "3.0.0-beta.6",
        "react-test-renderer": "16.0.0",
        "redux-mock-store": "1.3.0",
        "rimraf": "2.6.2",
        "transform-jest-deps": "2.2.1",
        "url-loader": "0.5.9",
        "webpack": "3.6.0",
        "webpack-bundle-analyzer": "2.9.0",
        "webpack-dev-middleware": "1.12.0",
        "webpack-hot-middleware": "2.19.1"
    },
    "dependencies": {
        "babel-polyfill": "6.26.0",
        "classnames": "2.2.5",
        "history": "4.7.2",
        "jss": "8.1.0",
        "jss-camel-case": "5.0.0",
        "jss-default-unit": "7.0.0",
        "jss-expand": "4.0.1",
        "jss-extend": "5.0.0",
        "jss-global": "2.0.0",
        "jss-isolate": "5.0.0-pre.2",
        "jss-nested": "5.0.0",
        "jss-props-sort": "5.0.0",
        "jss-vendor-prefixer": "6.0.0",
        "lodash._root": "3.0.1",
        "lodash.debounce": "4.0.8",
        "lodash.throttle": "4.1.1",
        "pace-progress": "1.0.2",
        "prop-types": "15.6.0",
        "ramda": "0.24.1",
        "raven-js": "3.17.0",
        "react": "16.0.0",
        "react-dom": "16.0.0",
        "react-jss": "7.2.0",
        "react-notification-system": "0.2.15",
        "react-redux": "5.0.6",
        "react-router-dom": "4.2.2",
        "react-router-redux": "5.0.0-alpha.6",
        "react-svg-inline": "2.0.0",
        "redux": "3.7.2",
        "redux-immutable-state-invariant": "2.0.0",
        "redux-saga": "0.15.6",
        "reselect": "3.0.1"
    }
}
