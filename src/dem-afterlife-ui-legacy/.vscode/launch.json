{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch chrome with debugger",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:60782",
            "webRoot": "${workspaceRoot}/src/app",
            "sourceMapPathOverrides": {
                "webpack:///src/app/*": "${webRoot}/*"
            }
        }
    ]
}